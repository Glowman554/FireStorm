{
  "name": "firestorm-vscode",
  "displayName": "firestorm-vscode",
  "description": "FireStorm language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "firestorm",
      "aliases": ["FireStorm", "firestorm"],
      "extensions": ["fl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "firestorm",
      "scopeName": "source.firestorm",
      "path": "./syntaxes/firestorm.tmLanguage.json"
    }]
  }
}
