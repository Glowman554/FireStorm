function(assembly) prints(str idk) -> void {
"
	mov rdi, rax
[extern puts]
	jmp puts
"
}

function(assembly) printc(chr idk) -> void {
"
	mov rdi, rax
[extern putchar]
	jmp putchar
"
}

function(assembly) printi(int idk) -> void {
"
	mov rdi, .fstr
	mov rsi, rax
	mov rax, 0
[extern printf]
	jmp printf

.fstr: db '%d', 10, 0
"
}
